{
  "name": "@thekie/dropbox-lite",
  "version": "1.0.1",
  "description": "Upload, download, and manage files in Dropbox with automatic OAuth token refresh.",
  "keywords": [
    "dropbox",
    "cloud-storage",
    "file-management",
    "oauth"
  ],
  "author": "thekie",
  "license": "MIT",
  "homepage": "https://www.dropbox.com/developers",
  "clawhub": {
    "requiredEnv": [
      "DROPBOX_APP_KEY",
      "DROPBOX_APP_SECRET",
      "DROPBOX_REFRESH_TOKEN"
    ],
    "optionalEnv": [
      "DROPBOX_ACCESS_TOKEN"
    ],
    "credentials": {
      "primary": "dropbox",
      "description": "Dropbox API app credentials and OAuth refresh token"
    },
    "dependencies": {
      "python": ["requests"]
    }
  }
}
